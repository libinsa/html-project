2.css

.sam{
    border: 2px dashed brown;
    padding:2em;
    margin: 2em;
    margin-right: 3em;
    outline: 4px solid black ;
    outline-offset:6px;
}

.samm{
    border: 8px solid violet;
    border-radius: 9px;
    height: 100px;
    width: 100px;
    outline: 4px dashed white;
    outline-offset: 5px;
    background-color: blanchedalmond;

}

header{
    background-color: aquamarine;
}

main{
    background-color: aquamarine;
}

